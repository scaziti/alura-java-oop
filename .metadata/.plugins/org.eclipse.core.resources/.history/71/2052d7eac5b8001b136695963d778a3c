package testes.gerais;

public class Animal {
	
	private String name;
	private String noise;
	
	public Animal(String name, String noise) {
		this.name = name;
		this.noise = noise;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getNoise() {
		return noise;
	}
	
	public void setNoise(String noise) {
		this.noise = noise;
	}

}
