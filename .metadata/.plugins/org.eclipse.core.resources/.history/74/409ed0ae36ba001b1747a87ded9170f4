import java.util.Arrays;

public class SaldoInsuficienteException {

	
	public static void main(String[] args) {
	
		final int x = 1;
		final int y =2;
		switch (x+y) {
			case x +1 :{
				System.out.println("A");
			}
			case 1:
				System.out.println("B");
			
			default:
				System.out.println("def");
				break;
			case y:
				System.out.println("C");
			
		}
		
		
		
	}
}
