package hackerRank;

import java.util.Scanner;

public class JavaBigInteger {
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		int a = scan.nextInt();
		int b = scan.nextInt();
		
		int sum = a + b;
		int times = a * b;
		
		System.out.println(sum);
		System.out.println(times);
		
		scan.close();
	}

}
